<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="382.0" prefWidth="347.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectnr2javapython.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="69.0" prefWidth="322.0">
         <children>
            <ComboBox fx:id="WyborDnia" layoutX="-1.0" layoutY="48.0" onAction="#DzienWybrany" prefWidth="150.0" promptText="Wybierz Dzień..." />
            <Button fx:id="WczytajButton" layoutX="222.0" layoutY="48.0" mnemonicParsing="false" onAction="#onWczytajButtonClick" prefHeight="26.0" prefWidth="92.0" text="Pokaż Zajęcia" />
            <Button fx:id="PomocButton" layoutX="-1.0" mnemonicParsing="false" onAction="#onHelpButtonClicked" text="Pomoc" />
         </children>
      </AnchorPane>
      <Label prefHeight="18.0" prefWidth="381.0" text="Zajęcia:" />
      <ListView fx:id="HarmonogramList" onMouseClicked="#handleMouseClicked" prefHeight="200.0" prefWidth="200.0" />
      <Label prefHeight="17.0" prefWidth="365.0" text="Detali zajęć:" />
      <ListView fx:id="DetaleList" prefHeight="200.0" prefWidth="200.0" />
   </children>
</VBox>
